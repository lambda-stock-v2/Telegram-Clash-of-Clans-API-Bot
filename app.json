{
    "name": "Telegram Clash of Clans API Bot",
    "description": "Clash of Clans API ile yap\u0131lm\u0131\u015f bilgi \u00e7ekme botu.",
    "repository": "https://github.com/lambda-stock/Telegram-Clash-of-Clans-API-Bot",
    "buildpacks": [
      {
        "url": "heroku/python"
      }
    ]
    "env": {
        "API_HASH": {
            "description": "@TeleORG_Bot",
            "required": "True"
        },
        "API_ID": {
            "description": "@TeleORG_Bot",
            "required": "True"
        },
        "BOT_TOKEN": {
            "description": "@Botfather",
            "required": "True"
        },
        "COC_API_TOKEN": {
            "description": "https://developer.clashofclans.com/",
            "required": "True"
        }
    },
    "stack": "heroku-20",
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    }
}
